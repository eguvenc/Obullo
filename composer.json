{
    "name": "obullo/obullo",
    "type": "library",
    "description": "Create none-mvc applications with Obullo and Zend components.",
    "keywords": [
        "obullo",
        "modular",
        "micro",
        "pages",
        "psr-7",
        "psr-11",
        "psr-15"
    ],
    "homepage": "http://obullo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Ersin Guvenc",
            "email": "eguvenc@gmail.com",
            "homepage": "http://obullo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Obullo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "App\\": "tests/App/"
        }
    },
    "require": {
        "php": "^7.0",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "obullo/router": "^1.0",
        "monolog/monolog": "^1.24",
        "zendframework/zend-stratigility": "^3.1",
        "zendframework/zend-db": "^2.9",
        "zendframework/zend-config": "^3.1",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-validator": "^2.12",
        "zendframework/zend-diactoros": "^1.7",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-session": "^2.8",
        "zendframework/zend-i18n-resources": "^2.5",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-view": "^2.11",
        "zendframework/zend-escaper": "^2.6",
        "phpunit/phpunit": "^4.8"
    }
}
