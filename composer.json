{
    "name": "obullo/obullo",
    "type": "library",
    "description": "Obullo is the name given to the core library created by combining many components that allow you to use the 'Obullo Pages' tool'.",
    "keywords": [
        "obullo",
        "modular",
        "framework",
        "pages",
        "psr-7",
        "psr-11",
        "psr-15"
    ],
    "homepage": "http://obullo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Ersin Guvenc",
            "email": "eguvenc@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Obullo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "App\\": "tests/src/App/",
            "Test\\": "tests/src/Test/"
        }
    },
    "require": {
        "php": "^7.1",
        "obullo/router": "^1.7.4",
        "laminas/laminas-diactoros": "^2.2",
        "laminas/laminas-stratigility": "^3.2",
        "laminas/laminas-http": "^2.11",
        "laminas/laminas-httphandlerrunner": "^1.1",
        "laminas/laminas-modulemanager": "^2.8",
        "laminas/laminas-config": "^3.3",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-view": "^2.11"
    },
    "require-dev": {
        "monolog/monolog": "^1.24",
        "doctrine/dbal": "^2.9",
        "phpunit/phpunit": "^4.8",
        "laminas/laminas-httphandlerrunner": "^1.1",
        "laminas/laminas-diactoros": "^2.2",
        "laminas/laminas-stratigility": "^3.2",
        "laminas/laminas-db": "^2.11",
        "laminas/laminas-config": "^3.3",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-validator": "^2.13",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-session": "^2.9",
        "laminas/laminas-i18n-resources": "^2.6",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-view": "^2.11",
        "laminas/laminas-escaper": "^2.6",
        "laminas/laminas-modulemanager": "^2.8",
        "obullo/router": "^1.7"
    }
}
